{
    "name": "antonythorpe/silvershop-productimages",
    "type": "silverstripe-vendormodule",
    "description": "SilverShop submodule that adds multiple product images to the Product Page with a thumbnail gallery & zooming",
    "keywords": [
        "Silverstripe",
        "SilverShop",
        "multiple images",
        "extended images"
    ],
    "require": {
        "php": "^8.1",
        "bummzack/sortablefile": "^2.2@stable",
        "silvershop/core": "^3.2.0",
        "silverstripe/cms": "^5",
        "silverstripe/vendor-plugin": "^2@stable"
    },
    "require-dev": {
        "cambis/silverstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AntonyThorpe\\SilverShopProductImages\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.*-dev"
        },
        "expose": [
            "client"
        ]
    },
    "license": "MIT"
}
